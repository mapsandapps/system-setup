	"Prop": {
		"prefix": "prop",
		"body": [
			"    propName: {",
			"      type: [String, Number],",
			"      required: true",
			"    }"
		]
	},
	"Arrow Function": {
		"prefix": "arrow function",
		"body": [
			"($1) => {",
			"  $2",
			"}"
		]
	},
	"Stringify Console": {
		"prefix": "stringify console",
		"body": [
